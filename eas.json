{
  "cli": {
    "version": ">= 7.8.3",
    "appVersionSource": "remote"
  },
  "build": {
    "staging-preview": {
      "channel": "staging-preview",
      "autoIncrement": false,
      "distribution": "internal"
    },
    "staging-store": {
      "channel": "staging-store",
      "autoIncrement": false,
      "distribution": "store"
    },
    "production-preview": {
      "channel": "production-preview",
      "autoIncrement": true,
      "distribution": "internal"
    },
    "production-store": {
      "channel": "production-store",
      "autoIncrement": true,
      "distribution": "store"
    }
  },
  "submit": {
    "production": {}
  }
}
